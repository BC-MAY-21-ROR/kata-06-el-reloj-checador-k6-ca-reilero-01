<%=form_with model: @companies do |f| %>       
  <div class="form-container">
    <div class="container_form">
      <div class="container_form">
        <%= f.label 'Name' %>
        <%= f.text_field :name, class:"form-title" %>
      </div>
      <div class="container_form">
        <%= f.label 'Address' %>
        <%= f.text_field :address, class:"form-title" %>               
      </div>
    </div>            
    <div class="form-group">
      <div class="col-md-12 text-center" >
        <%= f.submit "new", class:"button_header button_new"  %>
      </div>
    </div>  
  </div>
<%end%>
